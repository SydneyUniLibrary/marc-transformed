{
  "defaultSeverity": "error",
  "extends": "tslint:recommended",
  "jsRules": {},
  "rules": {
    "arrow-parens": [ true, "ban-single-arg-parens" ],
    "no-console": false,
    "no-consecutive-blank-lines": [ true, 4],
    "no-null-keyword": true,
    "prefer-const": false,
    "quotemark": [ true, "single", "avoid-escape" ],
    "semicolon": [ true, "never" ]
  },
  "rulesDirectory": []
}
